@{ Layout = "~/Views/Shared/_LayoutNEW.cshtml"; }

@model List<FamilyAchievement>

<div class="row">

    @foreach (FamilyAchievement cdd in Model)
    {

<div>
    <div class="col-sm-3">

        <img src="~/Achievement/@cdd.AchievePic" width="190" height="190">

    </div>
    <div class="col-sm-3">

        <label width="190"><b>@cdd.AchieveName</b></label>

    </div>
</div>}
</div>


<style>
    label {
        text-align: center;
        margin-left: 58px;
    }
</style>